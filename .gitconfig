[user]
	email = xxx@xxx.com
	name = zhufeng
	signingkey = GPGKEYID
[commit]
	gpgsign = true
[core]
	; editor = vim
[merge]
	; tool = vimdiff
[format]
	pretty = oneline
[gpg]
	program = gpg2
[pager]
	branch = false
	log = true
	diff = true
	commit = false
	show = true
[alias]
	logline = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[color]
	ui = auto
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	pager = true
